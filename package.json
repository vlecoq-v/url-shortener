{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "ts express postgres url-shortener",
  "main": "dist/index.ts",
  "scripts": {
    "dev:prisma:migrate": "npx prisma migrate dev --preview-feature --schema=./src/prisma/schema.prisma",
    "dev:server": "nodemon --watch './src/*.ts' --watch './src/**/*.prisma' --exec 'ts-node' src/server.ts",
    "dev": "npm-run-all dev:prisma:migrate dev:server",
    "start": "tsc && node dist/server.js",
    "lint": "eslint .",
    "lint.fix": "eslint . --fix",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@prisma/client": "^4.13.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22"
  },
  "prisma.schema": "src/prisma/schema.prisma"
}
